{
  "name": "nodejs-swagger",
  "version": "1.0.2",
  "main": "build/utils/index.js",
  "bin": "build/util/index.js",
  "repository": "https://github.com/maybteam/nodejs-swagger.git",
  "author": "kyoongdev",
  "license": "MIT",
  "scripts": {
    "clear": "node scripts/clear-build.js",
    "dev": "nodemon --watch '*.ts' --exec 'ts-node' -r tsconfig-paths/register ./src/index.ts",
    "build": "yarn clear && tsc -b tsconfig.build.json && node scripts/copy-build.js",
    "start": "nodemon --config nodemon.json",
    "prepublishOnly": "yarn build",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "test-sdk": "node --trace-warnings --unhandled-rejections=strict build/express/sdk/index.js ./src/example/controllers ./test-sdk"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.188",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/reflect-metadata": "^0.1.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "prettier-plugin-organize-imports": "^3.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  }
}
